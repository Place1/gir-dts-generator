    "@types/node": "^8.0.47",
    "@types/uppercamelcase": "^3.0.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "camelcase": "^4.1.0",
    "commander": "^2.12.2",
    "common-tags": "^1.6.0",
